{
  "name": "@svag/lib",
  "version": "1.0.0",
  "description": "A library for drawing SVGs with common methods.",
  "main": "build",
  "scripts": {
    "t": "zoroaster -a",
    "test": "yarn t test/spec",
    "test-build": "ALAMODE_ENV=test-build yarn test",
    "test-all": "yarn-s test test-build",
    "test-watch": "yarn test -w",
    "lint": "eslint .",
    "doc": "NODE_DEBUG=doc doc documentary -o README.md",
    "d": "NODE_DEBUG=doc doc src/index.js -g",
    "b": "alamode src -o build",
    "build": "yarn-s d b doc",
    "rec": "NODE_DEBUG=appshot appshot -T 23 -a Terminal -y 150 -f",
    "e": "node example",
    "example/": "yarn e example/example.js"
  },
  "files": [
    "build"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/svagco/lib.git"
  },
  "keywords": [
    "lib",
    "svag",
    "svg",
    "generation",
    "image",
    "web",
    "png",
    "jpeg",
    "gif",
    "vector",
    "graphics",
    "picture",
    "terminal",
    "chrome",
    "safari",
    "browser"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/svagco/lib/issues"
  },
  "homepage": "https://github.com/svagco/lib#readme",
  "devDependencies": {
    "alamode": "1.4.0",
    "documentary": "1.14.0",
    "eslint-config-artdeco": "1.0.1",
    "yarn-s": "1.1.0",
    "zoroaster": "3.0.3"
  }
}
